{
    "configSchemaVersion": "1.0.9",
    "deploymentMode": "dev",
    "apps": {
        "monitoring": {
            "defaultJobWarningDelayInSeconds": 180
        }
    },
    "db": {
        "mongodb": {
            "main": "mongodb://prodiguer-db-mongo-user:{PRODIGUER_DB_MONGO_USER_PASSWORD}@{PRODIGUER_DB_MONGO_HOST}/metrics"
        },
        "pgres": {
            "main": "postgresql://prodiguer_db_user:{PRODIGUER_DB_PGRES_USER_PASSWORD}@{PRODIGUER_DB_PGRES_HOST}/prodiguer"
        }
    },
    "mq": {
        "connections": {
            "libigcm": "amqps://libigcm-mq-user:{PRODIGUER_MQ_RABBIT_LIBIGCM_USER_PASSWORD}@{PRODIGUER_MQ_RABBIT_HOST}/prodiguer?connection_attempts=3&heartbeat_interval=3600&{PRODIGUER_MQ_RABBIT_SSL_OPTIONS}",
            "main": "amqps://prodiguer-mq-user:{PRODIGUER_MQ_RABBIT_USER_PASSWORD}@{PRODIGUER_MQ_RABBIT_HOST}/prodiguer?connection_attempts=3&heartbeat_interval=3600&{PRODIGUER_MQ_RABBIT_SSL_OPTIONS}"
        },
        "mail": {
            "deleteProcessed": false,
            "idleFaultRetryDelayInSeconds": 60,
            "imap": {
                "host": "courriel3.ipsl.jussieu.fr",
                "mailbox": "AMPQ-TEST-QUEUE",
                "mailbox_processed": "AMPQ-TEST-PROCESSED",
                "password": "{PRODIGUER_MQ_IMAP_PASSWORD}",
                "port": 993,
                "username": "superviseur"
            },
            "smtp": {
                "addresses": {
                    "prodiguer": "superviseur@ipsl.jussieu.fr",
                    "supervisor": "superviseur@ipsl.jussieu.fr"
                },
                "host": "courriel3.ipsl.jussieu.fr",
                "password": "{PRODIGUER_MQ_SMTP_PASSWORD}",
                "port": 587,
                "username": "superviseur"
            }
        }
    },
    "web": {
        "cookie_secret": "{PRODIGUER_WEB_API_COOKIE_SECRET}",
        "host": "{PRODIGUER_WEB_HOST}",
        "metric": {
            "cors_white_list": [
                "*"
            ]
        },
        "url": "{PRODIGUER_WEB_URL}",
        "websocketKeepAliveDelayInSeconds": 280
    }
}