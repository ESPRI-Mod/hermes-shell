#!/bin/bash

# ###############################################################
# Deactivates prodiguer-shell
# ###############################################################

unset PRODIGUER_ACTION
unset PRODIGUER_ACTION_ARG
unset PRODIGUER_ACTION_SUBARG1
unset PRODIGUER_ACTION_SUBARG2
unset PRODIGUER_ACTION_SUBARG3
unset PRODIGUER_ACTION_SUBARG4

unset PRODIGUER_SHELL_EXEC
unset PRODIGUER_SHELL_COMMAND
unset PRODIGUER_SHELL_COMMANDS
unset PRODIGUER_SHELL_COMMAND_TYPE
unset PRODIGUER_SHELL_COMMAND_TYPES

unset HERMES_DIR_BASH
unset HERMES_DIR_BACKUPS
unset HERMES_DIR_CERTS
unset HERMES_DIR_CONFIG
unset HERMES_DIR_DAEMONS
unset HERMES_DIR_DATA
unset HERMES_DIR_JOBS
unset HERMES_DIR_LOGS
unset HERMES_DIR_PYTHON
unset HERMES_DIR_REPOS
unset HERMES_DIR_TEMPLATES
unset HERMES_DIR_SERVER
unset HERMES_DIR_SERVER_TESTS
unset HERMES_DIR_TMP
unset HERMES_DIR_VENV

unset PRODIGUER_REPOS
unset PRODIGUER_VENVS
unset PRODIGUER_OPS_DIRS
